{
  "name": "blogger-editor",
  "version": "1.0.0",
  "description": "A editor for Blogger writer",
  "scripts": {
    "1-webpack-build-development": "webpack --mode development",
    "2-webpack-watch-development": "webpack --watch --mode development --display errors-only",
    "3 webpack-watch-build-production": "webpack --watch --mode production --display errors-only",
    "4 webpack-build-production": "webpack --mode production",
    "e0.run-development": "electron bin/electron/index-electron.js --mode development",
    "e1.run-production": "electron bin/electron/index-electron.js --mode production",
    "host": "node bin/host/host.js",
    "host-without-open": "node bin/host/host-without-open.js",
    "webpack-help": "webpack --help",
    "webpack-test-build-development": "webpack --mode development --config [tmp/webpack.config.js",
    "mocha": "mocha lib-for-link/src/**/*.mocha-test.js",
    "mocha-watch": "mocha --watch lib-for-link/src/**/*.mocha-test.js",
    "selenium-side-runner": "selenium-side-runner lib-for-link/src/**/*.side",
    "t1-webpack-build-development": "webpack --mode development --config test/webpack.config.js --display errors-only",
    "t2-webpack-watch-development": "webpack --watch --mode development --config test/webpack.config.js --display errors-only"
  },
  "repository": {
    "type": "git",
    "url": "git+https://pulipuli.chen@gmail.com@github.com/pulipulichen/Blogger-Editor.git"
  },
  "author": "Pulipuli Chen <blog@pulipuli.info>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pulipulichen/Blogger-Editor/issues"
  },
  "homepage": "https://github.com/pulipulichen/Blogger-Editor#readme",
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "accepts": "^1.3.7",
    "babel-polyfill": "^6.26.0",
    "cacache": "^12.0.3",
    "colors-cli": "^1.0.23",
    "cookie": "^0.4.0",
    "cross-spawn": "^6.0.5",
    "dayjs": "^1.8.12",
    "dayjs-plugin-utc": "^0.1.2",
    "debug": "^4.1.1",
    "depd": "^2.0.0",
    "destroy": "^1.0.4",
    "detect-browser": "^4.4.0",
    "electron-context-menu": "^0.12.1",
    "electron-dl": "^1.14.0",
    "electron-in-page-search": "^1.3.2",
    "electron-is-dev": "^1.1.0",
    "encodeurl": "^1.0.2",
    "escape-html": "^1.0.3",
    "etag": "^1.8.1",
    "file-saver": "^2.0.1",
    "find-cache-dir": "^3.0.0",
    "fresh": "^0.5.2",
    "hash-sum": "^2.0.0",
    "jquery": "^3.4.1",
    "js-yaml": "^3.13.1",
    "json5": "^2.1.0",
    "jszip": "^3.2.2",
    "jszip-utils": "^0.0.2",
    "loader-utils": "^1.2.3",
    "ms": "^2.1.2",
    "on-finished": "^2.3.0",
    "open": "^6.0.0",
    "parseurl": "^1.3.3",
    "postcss": "^7.0.18",
    "postcss-value-parser": "^4.0.2",
    "proxy-addr": "^2.0.5",
    "qs": "^6.7.0",
    "range-parser": "^1.2.1",
    "safe-buffer": "^5.1.2",
    "safer-buffer": "^2.1.2",
    "schema-utils": "^2.2.0",
    "serialize-javascript": "^2.1.0",
    "source-map": "^0.7.3",
    "type-is": "^1.6.18",
    "uglify-js": "^3.6.0",
    "unpipe": "^1.0.0",
    "uuid": "^3.3.2",
    "vue-fragment": "^1.5.1",
    "vuedraggable": "^2.23.2",
    "webpack-sources": "^1.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.6.0",
    "@kazupon/vue-i18n-loader": "^0.4.1",
    "@microsoft/loader-raw-script": "^1.2.151",
    "@vue/component-compiler-utils": "^1.3.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "electron": "^5.0.2",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "find-free-port": "^2.0.0",
    "jsonfile": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "open-browsers": "^1.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.10.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.9",
    "webpack-shell-plugin": "^0.5.0"
  },
  "build": {
    "appId": "chrome.webapp.shortcuts.creator",
    "compression": "maximum",
    "mac": {
      "category": "chrome.webapp.shortcuts.creator"
    },
    "win": {
      "icon": "app/imgs/icon.ico",
      "target": "portable"
    },
    "linux": {
      "icon": "app/imgs/icon256.png",
      "category": "Network",
      "target": "deb",
      "desktop": {
        "Icon": "/opt/chrome-webapp-shortcuts-creator/app/imgs/icon256.png"
      }
    },
    "portable": {
      "artifactName": "chrome-webapp-shortcuts-creator.exe"
    }
  },
  "files": [
    "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
    "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
    "!**/node_modules/*.d.ts",
    "!**/node_modules/.bin",
    "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
    "!.editorconfig",
    "!**/._*",
    "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
    "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
    "!**/{appveyor.yml,.travis.yml,circle.yml}",
    "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
    "main.js",
    "ipc.js",
    "app/*"
  ]
}
